          MODULE=ding
         VERSION=1.9
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=http://ftp.tu-chemnitz.de/pub/Local/urz/$MODULE/
      SOURCE_VFY=sha256:69a6c81fcf785a286c053a352f3201cc8671c72851615c4b707a434309b098e5
