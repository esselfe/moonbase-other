          MODULE=liburing
         VERSION=2.12
          SOURCE=$MODULE-$VERSION.tar.gz
      SOURCE_URL=https://github.com/axboe/liburing/archive/refs/tags
      SOURCE_VFY=sha256:f1d10cb058c97c953b4c0c446b11e9177e8c8b32a5a88b309f23fdd389e26370
